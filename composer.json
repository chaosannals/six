{
    "name": "chaosannals/six",
    "description": "yet a thinkphp 6 library.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "chaosannals",
            "email": "chaosannals@outlook.com"
        }
